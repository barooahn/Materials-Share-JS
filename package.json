    "connect-flash": "^0.1.1",
    "connect-multiparty": "^2.2.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.6",
    "express-session": "^1.15.6",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.4.0",
    "mailgun-js": "^0.22.0",
    "material-ui": "^0.20.2",
